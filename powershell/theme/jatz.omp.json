{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#D3D7CF",
          "foreground": "#000000",
          "leading_diamond": "\ue0b6",
          "style": "powerline",
          "template": " ï¡± ",
          "type": "os"
        },
        {
          "background": "#3465A4",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#7CA000",
          "foreground": "#003642",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "powerline",
          "template": " \ue5ff {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#FD6D91",
          "foreground": "#003642",
          "powerline_symbol": "\ue0b0 ",
          "style": "powerline",
          "template": "<#0077c2,transparent></>\uf489 {{ .Name }} <transparent,#0077c2></>",
          "type": "shell"
        },
        {
          "background": "#BA8A00",
          "foreground": "#003642",
          "powerline_symbol": "\ue0b0 ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": "{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end}} ",
          "type": "git"
        },
        {
          "background": "#306998",
          "foreground": "#d5ad42",
          "powerline_symbol": "\ue0b0 ",
          "style": "powerline",
          "type": "python",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
        },
        {
          "background": "#6CA35E",
          "foreground": "#303030",
          "style": "powerline",
          "powerline_symbol": "\ue0b0 ",
          "template": "\ue718 {{if .PackageManagerIcon }}{{.PackageManagerIcon}} {{ end }}{{ .Full}} ",
          "type": "node"
        },
        {
          "type": "npm",
          "style": "powerline",
          "powerline_symbol": "\ue0b0 ",
          "foreground": "#ffffff",
          "background": "#cc3838",
          "template": "\ue71e {{ .Full }} "
        },
       {
          "background": "#E13154",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0 ",
          "style": "powerline",
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "java"
        },
        {
          "background": "#06A4CE",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0 ",
          "style": "powerline",
          "template": "\ue798 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "dart"
        },
        {
          "background": "#1BD760",
          "foreground": "#003642",
          "powerline_symbol": "\ue0b0 ",
          "properties": {
            "paused_icon": "\uf8e3 ",
            "playing_icon": "\ue602 ",
            "stopped_icon": "\uf04d ",
            "track_separator": " - "
          },
          "style": "powerline",
          "template": "\uf9c6 {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} ",
          "type": "spotify"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#FFFFFF",
          "style": "powerline",
          "template": "   ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
